{
  "name": "ngx-vertex-demo-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ngx-vertex-demo-e2e/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["ngx-vertex-demo"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/ngx-vertex-demo-e2e/cypress.config.js",
        "devServerTarget": "ngx-vertex-demo:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "ngx-vertex-demo:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/ngx-vertex-demo-e2e/**/*.{js,ts}"]
      }
    }
  }
}
